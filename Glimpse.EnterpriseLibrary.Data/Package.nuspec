<?xml version="1.0"?>
<package >
  <metadata>
    <id>Glimpse.EnterpriseLibrary.Data</id>
    <version>1.0.2</version>
    <title>Glimpse EnterpriseLibrary Data</title>
    <authors>Sawan Ruparel</authors>
